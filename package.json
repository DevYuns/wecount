{
  "name": "svelte-routify-typescript",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "preview": "vite preview",
    "dev": "vite",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint src --ext .ts,.js,.svelte",
    "format": "prettier --write ./src/**/*.{js,svelte,html,ts}",
    "test": "jest",
    "generate": "prisma generate",
    "update-types": "npx openapi-typescript $SUPABASE_TYPE_URL --output src/types/supabase.ts",
    "migrate": "dotenv -e ./dotenv/.env -- prisma migrate deploy",
    "migrate:dev": "dotenv -e ./dotenv/.env -- prisma migrate dev",
    "migrate:deploy": "dotenv -e ./dotenv/.env -- prisma migrate deploy",
    "migrate:reset": "dotenv -e ./dotenv/.env -- prisma migrate reset",
    "studio": "dotenv -e ./dotenv/.env -- prisma studio"
  },
  "dependencies": {
    "@supabase/supabase-js": "^1.29.1",
    "browser-image-compression": "^1.0.17",
    "date-fns": "^2.28.0",
    "nanoid": "^3.1.32",
    "svelte-i18n": "^3.3.13",
    "svelte-infinite-loading": "^1.3.8",
    "svelte-theme": "^1.4.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.8",
    "@dooboo/eslint-config-svelte": "^0.0.7",
    "@prisma/client": "3.8.1",
    "@roxi/routify": "^3.0.0-next.69",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.34",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^3.0.0",
    "@types/jest": "^27.4.0",
    "babel-jest": "^27.4.6",
    "dotenv": "^12.0.3",
    "dotenv-cli": "^4.1.1",
    "eslint": "^8.6.0",
    "jest": "^27.4.7",
    "postcss": "^8.4.5",
    "postcss-cssnext": "^3.1.1",
    "postcss-custom-media": "^8.0.0",
    "postcss-import": "^14.0.2",
    "postcss-load-config": "^3.1.1",
    "postcss-nested": "^5.0.6",
    "postcss-plugin": "^1.0.0",
    "postcss-preset-env": "^7.2.3",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "prettier-plugin-svelte": "^2.6.0",
    "prisma": "^3.8.1",
    "svelte": "^3.46.2",
    "svelte-check": "^2.3.0",
    "svelte-jester": "^2.1.5",
    "svelte-preprocess": "^4.10.1",
    "ts-jest": "^27.1.3",
    "ts-node": "10.4.0",
    "typescript": "^4.5.4",
    "vite": "2.7.12"
  }
}
