{
  "name": "svelte-routify-typescript",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "preview": "vite preview",
    "dev": "vite",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint src --ext .ts,.js,.svelte",
    "format": "prettier --write ./src/**/*.{js,svelte,html,ts}",
    "test": "jest",
    "generate": "prisma generate",
    "update-types": "npx openapi-typescript $SUPABASE_TYPE_URL --output src/types/supabase.ts",
    "migrate": "dotenv -e ./dotenv/.env -- prisma migrate deploy",
    "migrate:dev": "dotenv -e ./dotenv/.env -- prisma migrate dev",
    "migrate:deploy": "dotenv -e ./dotenv/.env -- prisma migrate deploy",
    "migrate:reset": "dotenv -e ./dotenv/.env -- prisma migrate reset",
    "studio": "dotenv -e ./dotenv/.env -- prisma studio"
  },
  "dependencies": {
    "@supabase/supabase-js": "^1.35.3",
    "browser-image-compression": "^2.0.0",
    "date-fns": "^2.28.0",
    "nanoid": "^3.3.4",
    "svelte-i18n": "^3.4.0",
    "svelte-infinite-loading": "^1.3.8",
    "svelte-theme": "^1.4.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "@dooboo/eslint-config-svelte": "^0.0.7",
    "@prisma/client": "3.14.0",
    "@roxi/routify": "^3.0.0-next.69",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.46",
    "@testing-library/svelte": "^3.1.2",
    "@tsconfig/svelte": "^3.0.0",
    "@types/jest": "^27.5.1",
    "babel-jest": "^28.1.0",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "postcss": "^8.4.14",
    "postcss-cssnext": "^3.1.1",
    "postcss-custom-media": "^8.0.0",
    "postcss-import": "^14.1.0",
    "postcss-load-config": "^4.0.1",
    "postcss-nested": "^5.0.6",
    "postcss-plugin": "^1.0.0",
    "postcss-preset-env": "^7.7.0",
    "prettier": "^2.6.2",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-svelte": "^2.7.0",
    "prisma": "^3.14.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.1",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.6",
    "ts-jest": "^28.0.3",
    "ts-node": "10.8.0",
    "typescript": "^4.7.2",
    "vite": "2.7.12"
  }
}
